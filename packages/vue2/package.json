{
  "name": "ericox-vue2",
  "version": "0.0.0-alpha",
  "description": "Unstyled Vue.js Components",
  "main": "dist/ericox.common",
  "unpkg": "dist/ericox.min.js",
  "typings": "dist/ericox.common",
  "files": [
    "dist/*",
    "README.md"
  ],

  "exports": {
    "import": "./dist/ericox.common",
    "require": "./dist/ericox.common",
    "types": "./dist/index.d.ts"
  },

  "private": false,

  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run build:wc & npm run build:lib",
    "lint": "vue-cli-service lint",
    "build:lib": "vue-cli-service build --target lib --name ericox 'src/index.js'",
    "build:wc": "vue-cli-service build --target wc --name ericox 'src/components/**/*.vue'"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "core-js": "^3.6.5",
    "node-sass": "^7.0.1",
    "sass-loader": "^10.1.1",
    "vue": "^2.6.11",
    "vue-loader": "^17.0.0",
    "vue-template-compiler": "^2.6.11"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KingAdeCode/ericox.git"
  },
  "keywords": [
    "vue",
    "vuejs",
    "elastik",
    "ui"
  ],
  "author": "kingadecode",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KingAdeCode/ericox/issues"
  },
  "homepage": "https://github.com/KingAdeCode/ericox/#readme"
}
